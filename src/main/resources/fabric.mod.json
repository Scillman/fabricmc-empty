{
    "schemaVersion": 1,
    "id": "mymod",
    "version": "${version}",
    "name": "My Mod",
    "description": "My mod for Minecraft using Fabric",
    "authors": [
        "Scillman"
    ],
    "contact": {
        "homepage": "https://github.com/Scillman/fabricmc-empty/",
        "sources": "https://github.com/Scillman/fabricmc-empty/",
        "issues": "https://github.com/Scillman/fabricmc-empty/issues/"
    },
    "license": "MIT",
    "icon": "icon.png",
    "environment": "*",
    "entrypoints": {
        "main": [
            "com.mymod.ModMain"
        ]
    },
    "mixins": [
        "mymod.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=${fabric_loader_version}",
        "minecraft": ">=${minecraft_version}",
        "java": ">=17",
        "fabric-api": ">=${fabric_api_version}"
    },
    "accessWidener": "mymod.accesswidener"
}